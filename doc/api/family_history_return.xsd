<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" xmlns:altova="http://www.altova.com/xml-schema-extensions" elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<xs:element name="Family_history">
		<xs:annotation>
			<xs:documentation>Family history of a variant</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			
			<xs:attribute name="numfamilies" type="xs:int" use="required">
				<xs:annotation>
					<xs:documentation>The number of families showing this variant</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="numcases" type="xs:int" use="required">
				<xs:annotation>
					<xs:documentation>The number of cases showing this variant</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
</xs:schema>
